<h1>Editing Weekly Schedule</h1>
<%= form_tag update_individual_airport_weekly_schedules_path, :method => :put do %>
<table>
<% for weekly_schedule in @weekly_schedule %>
<tr>
<td>
<%= weekly_schedule.employee.first_name + " " + weekly_schedule.employee.last_name %>
</td>
<%= fields_for "schedule_ids[]", weekly_schedule do |f| %>
<%= render 'form', f: f %>
<% end %>
<% end %>
</table>
<%= submit_tag "Submit" %>
<% end %>


<%= link_to 'Back', weekly_schedules_path %>
